{
    "name": "zdcode2-vscode",
    "displayName": "ZDCode 2.x for VS Code",
    "description": "ZDCode 2.x syntax highlighting for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "gustavo6046",
    "author": {
        "name": "Gustavo R. Rehermann (Gustavo6046)",
        "email": "gugurehermann@gmail.com",
        "url": "https://sentientteam.freeforums.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Gustavo6046/zdvode2-vscode.git"
    },
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "zdcode2",
            "aliases": ["ZDCode 2.x", "zdcode2"],
            "extensions": [".zc2"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "zdcode2",
            "scopeName": "source.zdcode2",
            "path": "./syntaxes/zdcode.tmLanguage.json"
        }]
    }
}